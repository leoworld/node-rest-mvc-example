h2
  Users

ul
  for user in users
    li #{user.name.first} #{user.name.last} #{user._id}
    
p=errors

form(action='/users',method='post')
  label First Name:
  input(name='user[name_first]')
  br
  label Last Name:
  input(name='user[name_last]')
  br
  label Email:
  input(name='user[email]')
  br
  label Role:
  input(name='user[role]')
  br
  label Password:
  input(name='user[pass]')
  br
  input(type='submit',value='Create User')
  
script(src='//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js')